{
    "version": 2,
    "name": "sprint-challenge-react-wars",
    "builds": [
        {
            "src": "sprint-challenge-react-wars/package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "sprint-challenge-react-wars/$1"
        }
    ]
}